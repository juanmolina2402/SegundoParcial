<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.segundoparcial">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SegundoParcial">
        <activity
            android:name=".AddContactMCActivity"
            android:exported="false" />
        <activity
            android:name=".MainMCActivity"
            android:exported="true"
            android:label="Contactos" />
        <activity
            android:name=".WelcomeMCActivity"
            android:exported="true"
            android:label="Bienvenido" />
        <activity
            android:name=".SplashMCActivity"
            android:exported="true"
            android:theme="@style/SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>